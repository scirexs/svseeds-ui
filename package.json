{
	"name": "svseeds-ui",
	"version": "0.0.1",
	"description": "Svelte headless simple components as svelte file.",
	"main": "index.js",
	"files": [
		"components/**/*",
		"index.js"
	],
	"keywords": ["svelte", "headless", "components", "ui"],
	"author": "scirexs",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/scirexs/svseeds-ui.git"
	},
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build && npm run prepack",
		"preview": "vite preview --host",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"format": "eslint --fix .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run"
	},
	"type": "module",
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@stylistic/eslint-plugin": "^4.2.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"@testing-library/user-event": "^14.6.1",
		"eslint": "^9.18.0",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"publint": "^0.3.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0",
		"vitest": "^3.0.0"
	}
}
